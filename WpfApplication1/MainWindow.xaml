<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        mc:Ignorable="d"
        Title="MainWindow" Height="442.167" Width="463.667">
    <Grid Margin="160,239,160,30">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="89*"/>
            <ColumnDefinition Width="47*"/>
            <ColumnDefinition Width="62*"/>
        </Grid.ColumnDefinitions>

        <!-- input text box-->
        <TextBox Name="_Go" Margin="-120,-213,-119.687,142" Grid.ColumnSpan="3" Height="213" VerticalAlignment="Bottom" TextChanged="_Go_TextChanged" />
        <!-- GO button-->
        <Button   Click="Click_Go" Grid.ColumnSpan="3" Height="49" Margin="0,0,0.313,42" VerticalAlignment="Bottom">
            <TextBlock>Go</TextBlock>
        </Button>
        <TextBlock x:Name="outputText" Grid.ColumnSpan="3" HorizontalAlignment="Left" Height="37" Margin="-120,-62,-120,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="437"/>

    </Grid>
</Window>
